<view class="container wx-container"><view class="input-box"><input class="input" type="text" focus="{{true}}" placeholder="查找商品 如:苹果" data-event-opts="{{[['input',[['inputVal',['$event']]]],['confirm',[['searchGoods']]]]}}" value="{{searchVal}}" bindinput="__e" bindconfirm="__e"/><image class="icon" src="../../static/icon/search.png" data-event-opts="{{[['tap',[['searchGoods']]]]}}" bindtap="__e"></image></view><view class="history-box"><view class="title-box"><view class="title">历史记录</view><view data-event-opts="{{[['tap',[['delHistory',['$event']]]]]}}" class="icon-box" bindtap="__e"><uni-icons vue-id="707c8ac6-1" type="trash" size="24" color="#909399"></uni-icons></view></view><block s-if="{{history.length>0}}"><view class="history"><block s-for="history trackBy index" s-for-item="item" s-for-index="index"><block s-if="{{isShowHistory==false?index<10:index<history.length}}"><view data-event-opts="{{[['tap',[['searchGoods',['$0'],[[['history','',index]]]]]]]}}" class="item" bindtap="__e">{{item}}</view></block></block><block s-if="{{history.length>10}}"><view class="item"><block s-if="{{!isShowHistory}}"><uni-icons vue-id="707c8ac6-2" type="bottom" size="10" data-event-opts="{{[['^click',[['setShowHistory']]]]}}" bind:click="__e"></uni-icons></block></view></block></view></block><block s-else><view class="no">无历史记录</view></block></view><view class="goods-list wx-goods-list"><view class="title">搜索商品</view><block s-if="{{goodsList.length>0}}"><view class="goods-box"><block s-for="goodsList trackBy item.id" s-for-item="item" s-for-index="index"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['goodsList','id',item.id,'id']]]]]]]}}" class="goods-item" bindtap="__e"><view class="l"><image class="img" src="{{item.image}}"></image><view class="info"><view class="name line">{{item.title}}</view><view class="dec line2">{{item.describe}}</view><view class="price">{{"￥"+item.price+"/份"}}</view></view></view><view data-event-opts="{{[['tap',[['addCartGoods',['$0'],[[['goodsList','id',item.id,'id']]]]]]]}}" class="r" catchtap="__e"><image class="icon" src="../../static/icon/gouwuche.png"></image></view></view></block></view></block><block s-else><view class="no">无商品</view></block></view></view>