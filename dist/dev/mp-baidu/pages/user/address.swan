<view class="container"><block s-if="{{defaultAddress||basicAddress.length>0}}"><view class="list"><block s-if="{{defaultAddress}}"><view class="item"><view class="content"><view class="top"><view class="text1 line">{{defaultAddress.province+defaultAddress.city+defaultAddress.county+defaultAddress.address}}</view><view class="text2">默认</view></view><view class="bot">{{defaultAddress.name+' '+defaultAddress.mobile}}</view></view><image class="icon" src="../../static/icon/edit.png" data-event-opts="{{[['tap',[['editAddress',['$0'],['defaultAddress']]]]]}}" bindtap="__e"></image></view></block><block s-for="basicAddress" s-for-item="item" s-for-index="index"><block s-if="{{basicAddress.length>0}}"><view class="item"><view class="content"><view class="top"><view class="text1 line">{{item.province+item.city+item.county+item.address}}</view></view><view class="bot">{{item.name+' '+item.mobile}}</view></view><image class="icon" src="../../static/icon/edit.png" data-event-opts="{{[['tap',[['editAddress',['$0'],[[['basicAddress','',index]]]]]]]}}" bindtap="__e"></image></view></block></block></view></block><block s-else><view class="no-content"><view class="content"><image class="icon" src="../../static/icon/no_address.png"></image><view class="text">您还没有添加收货地址哦~</view></view></view></block></view>