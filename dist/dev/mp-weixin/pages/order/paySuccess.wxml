<view class="container"><block wx:if="{{status==1}}"><view class="info"><image class="icon" src="../../static/icon/success.png" mode></image><view class="text">支付成功</view></view></block><block wx:else><view class="info"><image class="icon" src="../../static/icon/fail.png" mode></image><view class="text">支付失败</view></view></block><view class="btn-box"><button data-event-opts="{{[['tap',[['toOrder',['$event']]]]]}}" class="btn btn1" bindtap="__e">查看订单</button><button data-event-opts="{{[['tap',[['toIndex',['$event']]]]]}}" class="btn btn2" bindtap="__e">返回首页</button></view></view>